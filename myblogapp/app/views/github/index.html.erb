  <div class="row" style="margin-top:20px">
   
    <div class="col-md-10"><h2>My Github Repos!</h2></div>
   
  </div>
   </br>

  <%  @repos_from_github.each do |repo| %>
 <ul class="card" style="background-color: #EBEBEB;font-size:20px; margin-bottom:20px;" >
  <li class="card-body" >
   <div class="col-md-12 " >
     <%= repo.name %>
   </div>
    <div class="col-md-12 " >
    <a href="#">   <%= repo.url %></a>
   </div>
    <div class="col-md-12 " >
     <%= repo.stargazers_count %> stars
   <div>
  </li>
  </ul>
   <% end %>

